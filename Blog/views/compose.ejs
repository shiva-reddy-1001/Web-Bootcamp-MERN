<%- include('./partials/header'); -%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <h3>Title</h3>
        <textarea name='newtitle' cols='155' rows="1" class="form-control" style="border-radius: 5px";></textarea>
        <h3>Post</h3>
        <textarea name='newdata' cols="155" rows="10" class="form-control" style="border-radius: 5px"></textarea>
    </div>
    <button type="submit" name='submit' class="btn btn-primary">Publish</button>
</form>
<%- include('./partials/footer'); -%>